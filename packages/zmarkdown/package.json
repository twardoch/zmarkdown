{
  "name": "zmarkdown",
  "version": "11.3.0",
  "description": "HTTP server API providing fast and extensible markdown parser",
  "keywords": [
    "markdown",
    "md",
    "http",
    "api"
  ],
  "author": "Victor Felder <victor@draft.li> (https://draft.li)",
  "contributors": [
    "Sébastien (AmarOk) Blin <contact@enconn.fr>",
